	height=&#34;487&#34;
	srcset=&#34;https://yukari.pages.dev/autobangumi-qbittorrent-jellyfin/image-15_hu11803137521829144993.png 1024w, https://yukari.pages.dev/autobangumi-qbittorrent-jellyfin/image-15_hu6834979888717765723.png 2048w&#34;
	loading=&#34;lazy&#34;
	
	
		class=&#34;gallery-image&#34; 
		data-flex-grow=&#34;250&#34;
		data-flex-basis=&#34;602px&#34;
	
